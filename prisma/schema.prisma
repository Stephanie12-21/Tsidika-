generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model newsletter {
  id               Int     @id @default(autoincrement())
  email            String  @unique
  unsubscribeToken String?

  @@map("newsletter")
}
